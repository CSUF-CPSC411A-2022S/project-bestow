<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <!-- Create a fragment element that displays the layout associated with a fragment. In this case
         we display the Profile fragment by setting the class name to the android:name attribute.
         We use match_parent for the layouts so the fragment fills the entire screen.
    -->
    <fragment
        android:id="@+id/main_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.example.testproject.Profile"
        />

</androidx.constraintlayout.widget.ConstraintLayout>